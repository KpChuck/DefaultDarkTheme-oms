<?xml version="1.0" encoding="utf-8"?>

<resources>

    <style name="SpeedDialtactsTheme" parent="android:Theme.Material">

        <!-- Styles that require AppCompat compatibility, remember to update both sets -->
        <item name="android:windowActionBarOverlay">true</item>
        <item name="@*com.android.dialer:attr/windowActionBarOverlay">true</item>
        <item name="android:windowActionModeOverlay">true</item>
        <item name="@*com.android.dialer:attr/windowActionModeOverlay">true</item>
        <item name="android:actionBarStyle">@*com.android.dialer:style/SpeedDialtactsActionBarStyle</item>
        <item name="@*com.android.dialer:attr/actionBarStyle">@*com.android.dialer:style/SpeedDialtactsActionBarStyle</item>
        <!-- Style for the overflow button in the actionbar. -->
        <item name="android:actionOverflowButtonStyle">@style/DialtactsActionBarOverflow</item>
        <item name="@*com.android.dialer:attr/actionOverflowButtonStyle">@style/DialtactsActionBarOverflow</item>

        <!--  Drawable for the back button -->
        <!--
        <item name="android:homeAsUpIndicator">@drawable/ic_back_arrow</item>
        -->
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:listViewStyle">@*com.android.dialer:style/ListViewStyle</item>
        <item name="android:overlapAnchor">true</item>
        <item name="android:alertDialogTheme">@style/AlertDialogTheme</item>
        <item name="@*com.android.dialer:attr/activated_background">@*com.android.dialer:drawable/list_item_activated_background</item>
        <item name="@*com.android.dialer:attr/section_header_background">@*com.android.dialer:drawable/list_title_holo</item>
        <item name="@*com.android.dialer:attr/list_section_header_height">32dip</item>
        <item name="@*com.android.dialer:attr/list_item_padding_top">12dp</item>
        <item name="@*com.android.dialer:attr/list_item_padding_right">24dp</item>
        <item name="@*com.android.dialer:attr/list_item_padding_bottom">12dp</item>
        <item name="@*com.android.dialer:attr/list_item_padding_left">16dp</item>
        <item name="@*com.android.dialer:attr/list_item_gap_between_image_and_text">15dp</item>
        <item name="@*com.android.dialer:attr/list_item_gap_between_label_and_data">8dip</item>
        <item name="@*com.android.dialer:attr/list_item_presence_icon_margin">4dip</item>
        <item name="@*com.android.dialer:attr/list_item_presence_icon_size">16dip</item>
        <item name="@*com.android.dialer:attr/list_item_photo_size">40dp</item>
        <item name="@*com.android.dialer:attr/list_item_profile_photo_size">70dip</item>
        <item name="@*com.android.dialer:attr/list_item_prefix_highlight_color">#ffffffff</item>
        <item name="@*com.android.dialer:attr/list_item_background_color">#ff303030</item>
        <item name="@*com.android.dialer:attr/list_item_header_text_indent">8dip</item>
        <item name="@*com.android.dialer:attr/list_item_header_text_color">#b3ffffff</item>
        <item name="@*com.android.dialer:attr/list_item_header_text_size">14sp</item>
        <item name="@*com.android.dialer:attr/list_item_header_height">30dip</item>
        <item name="@*com.android.dialer:attr/list_item_data_width_weight">5</item>
        <item name="@*com.android.dialer:attr/list_item_label_width_weight">3</item>
        <item name="@*com.android.dialer:attr/contact_browser_list_padding_left">0dp</item>
        <item name="@*com.android.dialer:attr/contact_browser_list_padding_right">0dp</item>
        <item name="@*com.android.dialer:attr/contact_browser_background">@*android:color/background_material_dark</item>
        <item name="@*com.android.dialer:attr/list_item_name_text_color">#ffffffff</item>
        <item name="@*com.android.dialer:attr/list_item_name_text_size">16sp</item>
        <item name="@*com.android.dialer:attr/list_item_text_indent">8dip</item>
        <item name="@*com.android.dialer:attr/list_item_text_offset_top">-2dp</item>
        <!-- CallLog -->
        <item name="@*com.android.dialer:attr/call_log_primary_text_color">#ffffffff</item>
        <item name="@*com.android.dialer:attr/call_log_primary_background_color">#000000</item>
        <item name="@*com.android.dialer:attr/call_log_secondary_text_color">#b3ffffff</item>
        <item name="@*com.android.dialer:attr/call_log_secondary_background_color">#333333</item>
        <item name="@*com.android.dialer:attr/call_log_header_color">#33b5e5</item>
        <!-- VoicemailStatus -->
        <item name="@*com.android.dialer:attr/call_log_voicemail_status_height">48dip</item>
        <item name="@*com.android.dialer:attr/call_log_voicemail_status_background_color">#262626</item>
        <item name="@*com.android.dialer:attr/call_log_voicemail_status_text_color">#888888</item>
        <item name="@*com.android.dialer:attr/call_log_voicemail_status_action_text_color">#33b5e5</item>
        <!-- Favorites -->
        <item name="@*com.android.dialer:attr/favorites_padding_bottom">?android:attr/actionBarSize</item>
        <item name="android:colorPrimary">#0288d1</item>
        <item name="android:colorPrimaryDark">#0277bd</item>
        <item name="@*com.android.dialer:attr/dialpad_key_button_touch_tint">#3303a9f4</item>
        <item name="android:colorControlActivated">#0288d1</item>
        <item name="android:colorButtonNormal">#0288d1</item>
        <!--
        <item name="android:textAppearanceButton">@style/DialerButtonTextStyle</item>
        -->

        <!-- Video call icon -->
        <item name="@*com.android.dialer:attr/list_item_video_call_icon_size">32dip</item>
        <item name="@*com.android.dialer:attr/list_item_video_call_icon_margin">8dip</item>
    </style>
</resources>
