<resources>

    <color name="colorPrimary">#ff212121</color>
    <color name="colorPrimaryDark">#ff000000</color>
    <color name="colorAccent">@android:color/holo_blue_bright</color>

    <style name="AppTheme" parent="@*projekt.substratum:style/Theme.AppCompat.NoActionBar">
        <item name="*projekt.substratum:attr/colorPrimary">#ff212121</item>
        <item name="*projekt.substratum:attr/colorPrimaryDark">#ff000000</item>
        <item name="*projekt.substratum:attr/colorAccent">@android:color/holo_blue_bright</item>
