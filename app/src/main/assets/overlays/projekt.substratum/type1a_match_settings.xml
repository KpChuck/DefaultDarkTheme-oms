        <item name="*projekt.substratum:attr/preferenceTheme">@*projekt.substratum:style/PreferenceThemeOverlay.v14.Material</item>
    </style>

    <style name="AppTheme.Dialog" parent="@*projekt.substratum:style/Theme.AppCompat.Dialog" >
        <item name="*projekt.substratum:attr/colorPrimary">#ff212121</item>
        <item name="*projekt.substratum:attr/colorPrimaryDark">#ff000000</item>
